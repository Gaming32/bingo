{
  "schemaVersion": 1,
  "id": "bingo",
  "version": "${version}",

  "name": "Bingo",
  "description": "A mod for playing bingo! Works best if on both client and server, but also has compat for Vanilla clients.",
  "authors": [
    "Gaming32",
    "Earthcomputer"
  ],
  "contact": {},

  "license": "MIT",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.gaming32.bingo.fabric.BingoFabric"
    ],
    "client": [
      "io.github.gaming32.bingo.client.fabric.BingoFabricClient"
    ],
    "fabric-datagen": [
      "io.github.gaming32.bingo.datagen.BingoDataGen"
    ],
    "jei_mod_plugin": [
      "io.github.gaming32.bingo.client.recipeviewer.jei.BingoJeiPlugin"
    ],
    "modmenu": [
      "io.github.gaming32.bingo.client.fabric.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "bingo-common.mixins.json",
    {
      "config": "bingo-common-client.mixins.json",
      "environment": "client"
    },
    "bingo-fabric.mixins.json",
    {
      "config": "bingo-fabric-client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.6",
    "fabric-api": ">=0.97.7",
    "minecraft": ">=1.20.5 <1.20.7"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_179": [
        "io/github/gaming32/bingo/ext/CriterionTriggerExt"
      ]
    }
  }
}
